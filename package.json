{
  "name": "backend-example-nestjs",
  "version": "0.6.0",
  "license": "MIT",
  "scripts": {
    "oidc-provider": "ts-node script/oidc-provider",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:open-api": "ts-node script/generate-open-api",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:sls": "nest build && sls offline",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.22",
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "^8.4.7",
    "@nestjs/graphql": "^10.0.22",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "apollo-server-express": "^3.10.1",
    "aws-lambda": "^1.0.7",
    "aws-serverless-express": "^3.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "graphql": "^16.6.0",
    "graphql-tools": "^8.3.3",
    "jwks-rsa": "^2.1.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^8.4.7",
    "@types/aws-lambda": "^8.10.102",
    "@types/aws-serverless-express": "^3.3.5",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.8",
    "@types/oidc-provider": "^7.11.1",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.12",
    "jest": "^28.1.3",
    "mongodb": "^3.7.3",
    "mongodb-memory-server": "^8.8.0",
    "oidc-provider": "^7.11.5",
    "prettier": "^2.7.1",
    "serverless-dotenv-plugin": "^4.0.2",
    "serverless-offline": "^9.2.5",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
